<h1>UserController#edit</h1>

<%= form_for :user, url: user_path(@user), method: :patch do |f| %>
  <p>
    <%= f.label :full_name %><br>
    <%= f.text_field :full_name %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :phone_number %><br>
    <%= f.text_field :phone_number %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', users_path %>
